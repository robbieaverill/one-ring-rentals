{
    "name": "highlight/demo",
    "description": "One Ring Rentals demo project for upgrading",
    "require": {
        "php": ">=5.6",
        "silverstripe/blog": "^3.2",
        "silverstripe-themes/simple": "^3.2",
        "silverstripe/recipe-cms": "4.2.2"
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "silverstripe/graphql-devtools": "dev-master"
    },
    "extra": {
        "expose": [
            "app/icons",
            "themes/one-ring/css",
            "themes/one-ring/js",
            "themes/one-ring/fonts",
            "themes/one-ring/images",
            "themes/one-ring/static"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    }
}
